<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="send_to_riskified">
                <argument name="data"
                          xsi:type="array">
                    <item name="config"
                          xsi:type="array">
                        <item name="type"
                              xsi:type="string">send_to_riskified</item>
                        <item name="label"
                              xsi:type="string"
                              translate="true">Send to Riskified</item>
                        <item name="url"
                              xsi:type="url"
                              path="riskified/riskified/masssend"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>